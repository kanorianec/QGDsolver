FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    T
    {
        solver smoothSolver;
        smoother FDIC;
        tolerance	1e-12;
        relTol		0;
        maxIter         2000;
    }
}

